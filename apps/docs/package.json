{
  "name": "docs",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "prepare": "panda codegen",
    "dev": "storybook dev -p 6006 --no-open",
    "prebuild": "panda codegen",
    "build": "storybook build --docs",
    "preview-storybook": "serve storybook-static",
    "lint": "eslint './src/**/*.{ts,tsx,mdx}'  --max-warnings 0",
    "lint:ci": "eslint './src/**/*.{ts,tsx,mdx}' --format junit -o reports/lint/js-lint-results.xml --max-warnings 0",
    "clean": "rm -rf node_modules && rm -rf storybook-static && rm -rf styled-system",
    "cache:clean": "rm -rf node_modules/.cache && rm -rf storybook-static && rm -rf styled-system"
  },
  "dependencies": {
    "@rypak/ui": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@eslint/js": "^9.25.0",
    "@pandacss/dev": "^0.53.5",
    "@rypak/eslint-config": "workspace:*",
    "@rypak/panda-preset": "workspace:*",
    "@rypak/prettier-config": "workspace:*",
    "@rypak/typescript-config": "workspace:*",
    "@storybook/addon-a11y": "^9.0.2",
    "@storybook/addon-docs": "^9.0.2",
    "@storybook/addon-onboarding": "^9.0.2",
    "@storybook/addon-vitest": "^9.0.2",
    "@storybook/react-vite": "^9.0.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.25.0",
    "globals": "^16.0.0",
    "playwright": "^1.52.0",
    "storybook": "^9.0.2",
    "turbo": "^2.3.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  }
}
